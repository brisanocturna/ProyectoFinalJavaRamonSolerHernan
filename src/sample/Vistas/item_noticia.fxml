<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="100.0" prefWidth="300.0" style="-fx-border-width: 2; -fx-border-color: #000000;" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controladores.ItemNoticia">
   <children>
      <HBox prefHeight="100.0" prefWidth="300.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <ImageView fx:id="imagenItemNoticia" fitHeight="98.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets top="2.0" />
               </HBox.margin>
            </ImageView>
            <VBox prefHeight="98.0" prefWidth="198.0">
               <children>
                  <TextField fx:id="tituloItemNoticia" editable="false">
                     <VBox.margin>
                        <Insets bottom="48.0" />
                     </VBox.margin>
                  </TextField>
                  <HBox prefHeight="25.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="autorItemNoticia" editable="false" />
                        <TextField fx:id="fechaItemNoticia" editable="false" />
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
               </children>
               <HBox.margin>
                  <Insets right="2.0" top="2.0" />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
